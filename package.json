{
  "name": "viewly-embed",
  "version": "1.0.0",
  "description": "Viewly embed video player",
  "repository": {
    "type": "git",
    "url": "https://github.com/Viewly/player.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.babel.js",
    "build": "cross-env NODE_ENV=production API webpack -p --config webpack.prod.babel.js",
    "build:staging": "cross-env NODE_ENV=production webpack -p --config webpack.prod.babel.js",
    "build:production": "cross-env NODE_ENV=production API=production webpack -p --config webpack.prod.babel.js",
    "deploy:staging": "npm run build:staging && ./deploy-staging.sh",
    "deploy:production": "cli-confirm 'This will build and deploy to production. Are you sure? (Y/n)' && npm run deploy:production:y || echo 'Aborted!'",
    "deploy:production:y": "npm run build:production && ./deploy-production.sh"
  },
  "author": "View.ly",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.5",
    "dotenv": "^4.0.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "next",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-config": "^7.4.0",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "clappr": "^0.2.97",
    "clappr-playback-speed": "git+https://github.com/Viewly/clappr-playback-speed.git",
    "clappr-thumbnails-plugin": "git+https://github.com/turshija/clappr-thumbnails-plugin.git",
    "dash-shaka-playback": "^2.2.6",
    "lodash": "^4.17.5",
    "prop-types": "^15.5.10",
    "query-string": "^5.1.0",
    "react": "^16.0.0",
    "react-css-modules": "^4.7.4",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
